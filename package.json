    "@mui/material": "^6.4.2",
    "@simplewebauthn/server": "^13.1.1",
    "@simplewebauthn/types": "^12.0.0",
    "@simplewebauthn/typescript-types": "^8.3.4",
    "@types/pg": "^8.11.11",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
