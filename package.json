    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.1",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.0.3",
